map := (container.ordered_map [ "Hund", "Katze", "Maus" ]
                              [ "dog",  "cat",  "mouse" ])

m := "mouse"
for x in map.values until m = x
  say "$m was found"
else
  say "$m not found"

a := "axolotl"
for x in map.values until a = x
  say "$a was found"
else
  say "$a not found"
