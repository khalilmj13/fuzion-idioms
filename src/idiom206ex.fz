ex206 is

  foo Function unit is ()->say "foo"
  bar Function unit is ()->say "bar"
  baz Function unit is ()->say "baz"
  barfl Function unit is ()->say "barfl"

  for str in ["bar",
              "foo",
              "barfl",
              "baz",
              "error" /* will cause a precondition failure! */]
  do
    ((ordered_map ["foo",   "bar",   "baz",   "barfl"  ]
                  [foo, bar, baz, barfl])[str].get.call)

