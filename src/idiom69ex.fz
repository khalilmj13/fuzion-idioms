ex69 is

  seed(s u64, f ()->unit) =>
    simple_random s f

  showRandom (msg String) =>
    for i in 0..5 do
      say "$msg random is {random.next_u32 100}"

  showRandom "default"

  seed 777 ()->
    showRandom "seeded with 777"

  time_seeded_random (()->
    showRandom "seeded with time")

  seed 777 ()->
    showRandom "seeded with 777"
