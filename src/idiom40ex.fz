ex40 is

  vertex(id i32) : property.orderable
  is
    # redefine `lteq` for parent feature's 'has_total_order' type feature
    fixed type.lteq    (a, b ex40.vertex) bool is a.id <= b.id
    fixed type.equality(a, b ex40.vertex) bool is a.id =  b.id

    redef as_string => "v$id"

  edge(a, b vertex) is

  graph(edges Sequence edge) is
    neighbors(v vertex) container.Set vertex  is
      for
        # NYI use (ps_set vertex).type.empty, see #152
        r := container.ps_set (list vertex).type.empty, if      (v = e.a) r.add e.b
                                                        else if (v = e.b) r.add e.a
                                                        else    r
        e in edges
      else
        r

  v1 := vertex 1
  v2 := vertex 2
  v3 := vertex 3
  v4 := vertex 4
  v5 := vertex 5
  vertices := [ v1; v2; v3; v4; v5 ]
  edges := [ edge v1 v2;
             edge v2 v4;
             edge v3 v4;
             edge v3 v5;
             edge v4 v5 ]
  g := graph edges

  say (g.neighbors v3)
