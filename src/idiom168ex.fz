ex168 isbyte

  doit(s, p string) =>
    t := if s.endsWith p
           s.substring 0 s.byteLength-p.byteLength
         else
           s
    say t

  doit "Mah Nà Mah Nà Do doo be-do-do" "Mah Nà "
  doit "Mah Nà Mah Nà Do doo be-do-do" "be-do-do"
  doit "Mah Nà Mah Nà Do doo be-do-do" "Do doo be-do-do"
